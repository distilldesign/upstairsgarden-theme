									.ug-figure
										.media
											%a.pull-left{href: '#'}
												%img.media-object.img-polaroid{src: 'library/images/flowers.jpg', alt: 'Orange Kalanchoe flowers are beautiful.'}
											.media-body
												%h4.media-heading Species Profile:  Sweet Pea
												%i Lathyrus odoratus
												%table.table.table-striped
													%tbody
														%tr
															%th	Type
															%td Annual Flowering Climber
														%tr
															%th Height
															%td 1 &ndash; 2 <small><i>m</i></small>
														%tr
															%th Light
															%td Full Sun
